{
  "name": "dev",
  "version": "1.0.0",
  "main": "n/a",
  "description": "dev-blog",
  "author": "Dany Dodson <hello@danydodson.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/danydodson/dev"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8000",
    "start": "yarn run develop",
    "lint": "eslint src --ext .js",
    "remark": "remark content/posts/",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "write-good": "write-good $(glob 'content/**/*.{md,mdx}')",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test-develop": "gatsby develop --port 8888",
    "test": "CYPRESS_SUPPORT=y start-server-and-test test-develop http://localhost:8888 cy:open",
    "test:ci": "start-server-and-test test-develop http://localhost:8888 cy:run",
    "build:gh": "gatsby build ----prefix-paths",
    "serve:gh": "gatsby serve --prefix-paths",
    "serve": "gatsby serve",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-markdown-style-guide"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint src --ext .js --fix"
    ]
  },
